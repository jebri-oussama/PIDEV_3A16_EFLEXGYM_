<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="370.0" prefWidth="371.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gestion_evenement.Controllers.AfficherTypeController">
    <children>
        <ImageView fitHeight="92.0" fitWidth="139.0">
            <image>
                <Image url="@logo.png" />
            </image>
        </ImageView>
        <Label layoutX="139.0" layoutY="30.0" text="Type List">
         <font>
            <Font name="System Bold" size="22.0" />
         </font></Label>

        <TableView fx:id="tableViewType" layoutX="66.0" layoutY="92.0" prefHeight="246.0" prefWidth="240.0">
            <columns>
                <TableColumn fx:id="idColumn" text="ID" />
                <TableColumn fx:id="typeNameColumn" text="Type" />
                <TableColumn fx:id="deleteColumn" text="Supprimer" />
            </columns>
        </TableView>
        <Button layoutX="66.0" layoutY="338.0" onAction="#redirectToAddType" text="Ajouter" />


    </children>
</AnchorPane>
